{
    "name": "a17",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve -o",
        "hmr": "ng serve --configuration hmr -o",
        "server": "tsc-watch --onSuccess \"node ./server/server.js\"  --onFailure \"echo Beep! Compilation Failed\" --compiler typescript/bin/tsc -p ./server/",
        "svg": "gulp svg",
        "build": "ng build --configuration production",
        "build:server": "webpack serve --open --entry / --output-path /dist --compress --content-base ./dist/a17 --history-api-fallback --port=8082",
        "test": "ng test",
        "lint": "eslint ./src --ext .ts,.js,.html",
        "lint:fix": "eslint ./src --ext .ts,.js,.html --fix",
        "e2e": "ng e2e",
        "report": "webpack-bundle-analyzer dist/a17/stats.json"
    },
    "private": true,
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "dependencies": {
        "@angular/animations": "~17.2.4",
        "@angular/cdk": "~17.2.2",
        "@angular/common": "~17.2.4",
        "@angular/compiler": "~17.2.4",
        "@angular/core": "~17.2.4",
        "@angular/forms": "~17.2.4",
        "@angular/platform-browser": "~17.2.4",
        "@angular/platform-browser-dynamic": "~17.2.4",
        "@angular/router": "~17.2.4",
        "@iharbeck/ngx-virtual-scroller": "^15.0.0",
        "@netcracker/ux-assets": "^1.2.0",
        "@netcracker/ux-ng2": "6.0.0",
        "brace": "0.11.1",
        "hammerjs": "^2.0.8",
        "moment": "2.29.4",
        "netmask": "^2.0.2",
        "ngx-color-picker": "^12.0.0",
        "resize-observer-polyfill": "^1.5.1",
        "rxjs": "~7.8.0",
        "tslib": "^2.4.1",
        "zone.js": "~0.14.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~17.2.3",
        "@angular-eslint/builder": "~17.2.1",
        "@angular-eslint/eslint-plugin": "~17.2.1",
        "@angular-eslint/eslint-plugin-template": "~17.2.1",
        "@angular-eslint/schematics": "~17.2.1",
        "@angular-eslint/template-parser": "~17.2.1",
        "@angular/cli": "~17.2.3",
        "@angular/compiler-cli": "~17.2.4",
        "@angular/language-service": "~17.2.4",
        "@angularclass/hmr": "^3.0.0",
        "@types/express": "^4.17.16",
        "@types/jasmine": "^4.3.1",
        "@types/jasminewd2": "^2.0.10",
        "@types/node": "^18.19.3",
        "@typescript-eslint/eslint-plugin": "6.16.0",
        "@typescript-eslint/parser": "6.16.0",
        "body-parser": "^1.19.0",
        "codelyzer": "^6.0.0",
        "es5-ext": "0.10.53",
        "eslint": "^8.40.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-rxjs": "^5.0.3",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "express": "^4.18.2",
        "gulp": "^4.0.2",
        "gulp-inject": "^5.0.5",
        "gulp-rename": "^2.0.0",
        "gulp-svgmin": "^3.0.0",
        "gulp-svgstore": "^7.0.1",
        "husky": "^4.3.8",
        "jasmine-core": "~4.5.0",
        "jasmine-spec-reporter": "~5.0.2",
        "karma": "~6.4.1",
        "karma-chrome-launcher": "~3.1.1",
        "karma-coverage": "~2.0.3",
        "karma-coverage-istanbul-reporter": "^3.0.3",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "^2.0.0",
        "prettier": "^2.8.3",
        "pretty-quick": "^2.0.2",
        "protractor": "~7.0.0",
        "ts-node": "~10.9.1",
        "tsc-watch": "^4.2.9",
        "tslint": "~6.1.0",
        "typescript": "~5.2.2"
    }
}
